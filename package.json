{
  "name": "countries-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts,vue}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.7",
    "lazysizes": "^5.3.2",
    "material-design-icons-iconfont": "^6.7.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuetify": "^3.x"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "jest-environment-jsdom": "^29.7.0",
    "sass-embedded": "^1.81.0",
    "ts-jest": "^29.0.0",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-jest": "^4.0.1",
    "vue-tsc": "^2.1.8",
    "vuetify": "^3.7.4"
  }
}
